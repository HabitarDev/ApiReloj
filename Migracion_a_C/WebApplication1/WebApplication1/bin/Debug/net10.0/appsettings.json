{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=apireloj;Username=apireloj;Password=apireloj"
  },
  "JornadaProcessing": {
    "WorkerIntervalMinutes": 5,
    "IncompleteTimeoutHours": 24,
    "AttendanceStatusMap": {
      "CheckIn": [ "checkIn" ],
      "BreakIn": [ "breakIn" ],
      "BreakOut": [ "breakOut" ],
      "CheckOut": [ "checkOut" ]
    }
  },
  "BackfillPolling": {
    "WorkerIntervalMinutes": 30,
    "WindowMinutes": 30,
    "MaxResultsPerPage": 30,
    "HttpTimeoutSeconds": 30,
    "RunOnStartup": true,
    "BootstrapStartUtc": "2000-01-01T00:00:00Z",
    "MaxWindowsPerRun": 5000
  }
}
